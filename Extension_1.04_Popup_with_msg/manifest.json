{
   "manifest_version": 2,
   "name": "Working with the browser",
   "version": "0.2",
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["content.js"]
      }
   ],
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_popup": "popup.html"
   }
}